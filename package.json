{
    "name": "generator-minecraft-addon-devbed",
    "version": "0.0.0",
    "main": "generators/index.js",
    "repository": "https://github.com/Richienb/generator-minecraft-addon-devbed.git",
    "author": "Richie Bendall <richiebendall@gmail.com>",
    "license": "MIT",
    "files": [
        "generators"
    ],
    "keywords": [
        "minecraft",
        "bedrock",
        "addon",
        "add-on",
        "mcpe",
        "yeoman-generator"
    ],
    "scripts": {
        "test": "jest",
        "lint": "eslint ."
    },
    "dependencies": {
        "chalk": "^2.4.2",
        "uuid": "^3.3.3",
        "yeoman-generator": "^4.0.1",
        "yosay": "^2.0.1"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^2.0.0",
        "@typescript-eslint/parser": "^2.16.0",
        "eslint": "^6.2.2",
        "eslint-config-google": "^0.13.0",
        "husky": "^3.0.4",
        "jest": "^24.9.0",
        "prettier": "^1.18.2",
        "typescript": "^3.6.2",
        "yeoman-assert": "^3.1.0",
        "yeoman-test": "^2.0.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "eslint ."
        }
    },
    "engines": {
        "npm": ">= 4.0.0"
    },
    "jest": {
        "testEnvironment": "node"
    },
    "eslintConfig": {
        "env": {
            "browser": true,
            "es6": true,
            "node": true
        },
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint"
        ],
        "extends": [
            "google"
        ],
        "rules": {
            "indent": [
                "error",
                4
            ],
            "quotes": [
                "error",
                "double"
            ],
            "semi": [
                "error",
                "never"
            ],
            "brace-style": 0,
            "max-len": 0,
            "valid-jsdoc": 0,
            "spaced-comment": 0,
            "object-curly-spacing": 0,
            "require-jsdoc": 0
        }
    }
}
